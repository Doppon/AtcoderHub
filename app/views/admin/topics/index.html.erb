<div class="container">
  <h1>Admin::Topics#index</h1>
  <p>Find me in app/views/admin/topics/index.html.erb</p>

  <table class="table">
    <thead>
      <tr>
        <th>id</th>
        <th>title</th>
        <th>created_at</th>
      </tr>
    </thead>
    <tbody>
      <% @topics.each do |topic| %>
      <tr>
        <td><%= topic.id %></td>
        <td><%= topic.title %></td>
        <td><%= topic.created_at %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
  <div class="text-center">
    <%= link_to "HOME", admin_home_index_path %>
  </div>
</div>
